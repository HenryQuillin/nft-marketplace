{
  "root": true,
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": [
    "plugin:react/recommended",
    "airbnb",
    "plugin:prettier/recommended"
  ],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "plugins": [
    "react"
  ],

  "rules": {
    "import/order":"off",
    "react/jsx-props-no-spreading":0,
    "no-unused-vars":"warn",
    "react/jsx-no-undef":"warn",
    "prettier/prettier": [1, {"singleQuote": false,"semi":false}],
  "no-restricted-imports":"off",
    "react/prop-types":0,
    "react/jsx-filename-extension": [
      1,
      {
        "extensions": [
          ".js",
          ".jsx"
        ]
      }
    ]
  }
}
